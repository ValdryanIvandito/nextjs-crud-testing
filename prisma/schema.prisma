datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model cancellation_history {
  id         String @id @default(auto()) @map("_id") @db.ObjectId
  bookingref String @unique
  startsAt   String
  endsAt     String
  firstName  String
  email      String
}
